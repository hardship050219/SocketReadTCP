<Application
	xmlns="https://github.com/avaloniaui"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	x:Class="AppZC.UI.AvaApp"
	RequestedThemeVariant="Default">
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceInclude Source="/UI/Assets/Icons.axaml" />
				<ResourceInclude Source="avares://ZC.Core.UI.Theme/_index.axaml" />
			</ResourceDictionary.MergedDictionaries>
			<EnumEqualsConverter x:Key="EnumEqualsConverter" />
		</ResourceDictionary>
	</Application.Resources>
	<Application.Styles>
		<StyleInclude Source="avares://IconPacks.Avalonia/Icons.axaml" />
		<StyleInclude Source="avares://ZC.Core.UI.Theme/Styles/_index.axaml" />
		<UiTheme Locale="zh-CN" />
	</Application.Styles>
</Application>