				<Setter Property="Foreground" Value="{DynamicResource Label.TagGhostCyanForeground}" />
			</Style>
			<Style Selector="^.Teal">
				<Setter Property="BorderBrush" Value="{DynamicResource Label.TagGhostTealBorderBrush}" />
				<Setter Property="Foreground" Value="{DynamicResource Label.TagGhostTealForeground}" />
			</Style>
			<Style Selector="^.Green">
