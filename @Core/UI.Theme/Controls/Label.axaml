				<Setter Property="Foreground" Value="{DynamicResource Label.TagGhostLightGreenForeground}" />
			</Style>
			<Style Selector="^.Lime">
				<Setter Property="BorderBrush" Value="{DynamicResource Label.TagGhostLimeBorderBrush}" />
				<Setter Property="Foreground" Value="{DynamicResource Label.TagGhostLimeForeground}" />
			</Style>
			<Style Selector="^.Yellow">
