		</Style>

		<Style Selector="^.White">
			<Setter Property="Foreground" Value="{DynamicResource Label.TagSolidWhiteForeground}" />
			<Setter Property="BorderBrush" Value="{DynamicResource Label.TagSolidWhiteBorderBrush}" />
			<Setter Property="Background" Value="{DynamicResource Label.TagSolidWhiteBackground}" />
