	xmlns="https://github.com/avaloniaui"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	x:CompileBindings="True">
	<Design.PreviewWith>
		<StackPanel
			Width="500"
			Margin="20"
			HorizontalAlignment="Left"
			Spacing="20">
			<StackPanel.Styles>
				<Style Selector="Label">
					<Setter Property="Margin" Value="4" />
				</Style>
			</StackPanel.Styles>
			<WrapPanel>
				<Label Theme="{StaticResource Label-Tag}">Label</Label>
				<Label Classes="Large" Theme="{StaticResource Label-Tag}">Large Label</Label>
				<Label Classes="Circle" Theme="{StaticResource Label-Tag}">Circle Label</Label>
				<Label Classes="Large Circle" Theme="{StaticResource Label-Tag}">Large Circle Label</Label>
			</WrapPanel>
			<WrapPanel>
				<Label Classes="Red" Theme="{StaticResource Label-Tag}">Red</Label>
				<Label Classes="Pink" Theme="{StaticResource Label-Tag}">Pink</Label>
				<Label Classes="Purple" Theme="{StaticResource Label-Tag}">Purple</Label>
				<Label Classes="Violet" Theme="{StaticResource Label-Tag}">Violet</Label>
				<Label Classes="Indigo" Theme="{StaticResource Label-Tag}">Indigo</Label>
				<Label Classes="Blue" Theme="{StaticResource Label-Tag}">Blue</Label>
				<Label Classes="LightBlue" Theme="{StaticResource Label-Tag}">LightBlue</Label>
				<Label Classes="Cyan" Theme="{StaticResource Label-Tag}">Cyan</Label>
				<Label Classes="Teal" Theme="{StaticResource Label-Tag}">Teal</Label>
				<Label Classes="Green" Theme="{StaticResource Label-Tag}">Green</Label>
				<Label Classes="LightGreen" Theme="{StaticResource Label-Tag}">LightGreen</Label>
				<Label Classes="Lime" Theme="{StaticResource Label-Tag}">Lime</Label>
				<Label Classes="Yellow" Theme="{StaticResource Label-Tag}">Yellow</Label>
				<Label Classes="Amber" Theme="{StaticResource Label-Tag}">Amber</Label>
				<Label Classes="Orange" Theme="{StaticResource Label-Tag}">Orange</Label>
				<Label Classes="Grey" Theme="{StaticResource Label-Tag}">Grey</Label>
				<Label Classes="White" Theme="{StaticResource Label-Tag}">White</Label>
			</WrapPanel>
			<WrapPanel>
				<Label Classes="Ghost Red" Theme="{StaticResource Label-Tag}">Red</Label>
				<Label Classes="Ghost Pink" Theme="{StaticResource Label-Tag}">Pink</Label>
				<Label Classes="Ghost Purple" Theme="{StaticResource Label-Tag}">Purple</Label>
				<Label Classes="Ghost Violet" Theme="{StaticResource Label-Tag}">Violet</Label>
				<Label Classes="Ghost Indigo" Theme="{StaticResource Label-Tag}">Indigo</Label>
				<Label Classes="Ghost Blue" Theme="{StaticResource Label-Tag}">Blue</Label>
				<Label Classes="Ghost LightBlue" Theme="{StaticResource Label-Tag}">LightBlue</Label>
				<Label Classes="Ghost Cyan" Theme="{StaticResource Label-Tag}">Cyan</Label>
				<Label Classes="Ghost Teal" Theme="{StaticResource Label-Tag}">Teal</Label>
				<Label Classes="Ghost Green" Theme="{StaticResource Label-Tag}">Green</Label>
				<Label Classes="Ghost LightGreen" Theme="{StaticResource Label-Tag}">LightGreen</Label>
				<Label Classes="Ghost Lime" Theme="{StaticResource Label-Tag}">Lime</Label>
				<Label Classes="Ghost Yellow" Theme="{StaticResource Label-Tag}">Yellow</Label>
				<Label Classes="Ghost Amber" Theme="{StaticResource Label-Tag}">Amber</Label>
				<Label Classes="Ghost Orange" Theme="{StaticResource Label-Tag}">Orange</Label>
				<Label Classes="Ghost Grey" Theme="{StaticResource Label-Tag}">Grey</Label>
				<Label Classes="Ghost White" Theme="{StaticResource Label-Tag}">White</Label>
			</WrapPanel>
			<WrapPanel>
				<Label Classes="Solid Red" Theme="{StaticResource Label-Tag}">Red</Label>
				<Label Classes="Solid Pink" Theme="{StaticResource Label-Tag}">Pink</Label>
				<Label Classes="Solid Purple" Theme="{StaticResource Label-Tag}">Purple</Label>
				<Label Classes="Solid Violet" Theme="{StaticResource Label-Tag}">Violet</Label>
				<Label Classes="Solid Indigo" Theme="{StaticResource Label-Tag}">Indigo</Label>
				<Label Classes="Solid Blue" Theme="{StaticResource Label-Tag}">Blue</Label>
				<Label Classes="Solid LightBlue" Theme="{StaticResource Label-Tag}">LightBlue</Label>
				<Label Classes="Solid Cyan" Theme="{StaticResource Label-Tag}">Cyan</Label>
				<Label Classes="Solid Teal" Theme="{StaticResource Label-Tag}">Teal</Label>
				<Label Classes="Solid Green" Theme="{StaticResource Label-Tag}">Green</Label>
				<Label Classes="Solid LightGreen" Theme="{StaticResource Label-Tag}">LightGreen</Label>
				<Label Classes="Solid Lime" Theme="{StaticResource Label-Tag}">Lime</Label>
				<Label Classes="Solid Yellow" Theme="{StaticResource Label-Tag}">Yellow</Label>
				<Label Classes="Solid Amber" Theme="{StaticResource Label-Tag}">Amber</Label>
				<Label Classes="Solid Orange" Theme="{StaticResource Label-Tag}">Orange</Label>
				<Label Classes="Solid Grey" Theme="{StaticResource Label-Tag}">Grey</Label>
				<Label Classes="Solid White" Theme="{StaticResource Label-Tag}">White</Label>
