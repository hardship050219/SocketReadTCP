<ResourceDictionary
	xmlns="https://github.com/avaloniaui"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	x:CompileBindings="True">
	<StaticResource x:Key="{x:Type RepeatButton}" ResourceKey="{x:Type Button}" />
	<StaticResource x:Key="RepeatButton-Solid" ResourceKey="Button-Solid" />
	<StaticResource x:Key="RepeatButton-Outline" ResourceKey="Button-Outline" />
	<StaticResource x:Key="RepeatButton-Borderless" ResourceKey="Button-Borderless" />
</ResourceDictionary>