<ResourceDictionary xmlns="https://github.com/avaloniaui" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<Color x:Key="Color-Window">#000000</Color>
	<Color x:Key="Color-WindowText">#FFFFFF</Color>
	<Color x:Key="Color-Hotlight">#8080FF</Color>
	<Color x:Key="Color-GrayText">#A6A6A6</Color>
	<Color x:Key="Color-HighlightText">#2B2B2B</Color>
	<Color x:Key="Color-Highlight">#D6B4FD</Color>
	<Color x:Key="Color-ButtonText">#FFEE32</Color>
	<Color x:Key="Color-ButtonFace">#000000</Color>
	<SolidColorBrush x:Key="Brush-Window" Color="{StaticResource Color-Window}" />
	<SolidColorBrush x:Key="Brush-WindowText" Color="{StaticResource Color-WindowText}" />
	<SolidColorBrush x:Key="Brush-Hotlight" Color="{StaticResource Color-Hotlight}" />
	<SolidColorBrush x:Key="Brush-GrayText" Color="{StaticResource Color-GrayText}" />
	<SolidColorBrush x:Key="Brush-HighlightText" Color="{StaticResource Color-HighlightText}" />
	<SolidColorBrush x:Key="Brush-Highlight" Color="{StaticResource Color-Highlight}" />
	<SolidColorBrush x:Key="Brush-ButtonText" Color="{StaticResource Color-ButtonText}" />
	<SolidColorBrush x:Key="Brush-ButtonFace" Color="{StaticResource Color-ButtonFace}" />
	<ResourceDictionary.MergedDictionaries>
		<ResourceInclude Source="_index.axaml" />
	</ResourceDictionary.MergedDictionaries>
</ResourceDictionary>