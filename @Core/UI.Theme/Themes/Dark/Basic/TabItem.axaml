<ResourceDictionary xmlns="https://github.com/avaloniaui" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<!--  Line  -->
	<SolidColorBrush x:Key="TabItem.LinePipeBackground" Color="Transparent" />
	<StaticResource x:Key="TabItem.LinePipeSelectedBackground" ResourceKey="Brush-Primary" />
	<StaticResource x:Key="TabItem.LinePipePointeroverBorderBrush" ResourceKey="Brush-Fill1" />
	<StaticResource x:Key="TabItem.LinePipePressedBorderBrush" ResourceKey="Brush-Fill2" />
	<StaticResource x:Key="TabItem.LineHeaderForeground" ResourceKey="Brush-Text2" />
	<StaticResource x:Key="TabItem.LineHeaderPointeroverForeground" ResourceKey="Brush-Text0" />
	<StaticResource x:Key="TabItem.LineHeaderSelectedForeground" ResourceKey="Brush-Text0" />
	<StaticResource x:Key="TabItem.LineHeaderSelectedBackground" ResourceKey="Brush-PrimaryLight" />
	<StaticResource x:Key="TabItem.LineHeaderPointeroverBackground" ResourceKey="Brush-Fill0" />
	<StaticResource x:Key="TabItem.LineHeaderPressedBackground" ResourceKey="Brush-Fill1" />
	<!--  Card  -->
	<StaticResource x:Key="TabItem.CardHeaderSelectedBackground" ResourceKey="Brush-Background0" />
	<StaticResource x:Key="TabItem.CardHeaderPointeroverBackground" ResourceKey="Brush-Fill0" />
	<StaticResource x:Key="TabItem.CardHeaderPressedBackground" ResourceKey="Brush-Fill1" />
	<!--  Button  -->
	<StaticResource x:Key="TabItem.ButtonHeaderSelectedForeground" ResourceKey="Brush-Primary" />
	<StaticResource x:Key="TabItem.ButtonHeaderSelectedBackground" ResourceKey="Brush-PrimaryLight" />
	<StaticResource x:Key="TabItem.ButtonHeaderPointeroverBackground" ResourceKey="Brush-Fill0" />
	<StaticResource x:Key="TabItem.ButtonHeaderPressedBackground" ResourceKey="Brush-Fill1" />
</ResourceDictionary>