<ResourceDictionary xmlns="https://github.com/avaloniaui" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<StaticResource x:Key="ProgressBar.IndicatorBrush" ResourceKey="Brush-Primary" />
	<StaticResource x:Key="ProgressBar.Background" ResourceKey="Brush-Fill0" />
	<StaticResource x:Key="ProgressBar.TextForeground" ResourceKey="Brush-Text0" />
	<StaticResource x:Key="ProgressBar.OuterTextForeground" ResourceKey="Brush-Text0" />
	<SolidColorBrush x:Key="ProgressBar.RootBorderBrush" Color="Transparent" />
	<StaticResource x:Key="ProgressBar.PrimaryForeground" ResourceKey="Brush-Primary" />
	<StaticResource x:Key="ProgressBar.SecondaryForeground" ResourceKey="Brush-Secondary" />
	<StaticResource x:Key="ProgressBar.TertiaryForeground" ResourceKey="Brush-Tertiary" />
	<StaticResource x:Key="ProgressBar.SuccessForeground" ResourceKey="Brush-Success" />
	<StaticResource x:Key="ProgressBar.WarningForeground" ResourceKey="Brush-Warning" />
	<StaticResource x:Key="ProgressBar.DangerForeground" ResourceKey="Brush-Danger" />
	<!-- Error style is obsolete, use Danger instead -->
	<StaticResource x:Key="ProgressBarErrorForeground" ResourceKey="Brush-Danger" />
</ResourceDictionary>