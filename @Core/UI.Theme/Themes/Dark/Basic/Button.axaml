	<StaticResource x:Key="Button.OutlinePressedBackground" ResourceKey="Brush-Fill1" />
	<StaticResource x:Key="Button.OutlineSuccessBorderBrush" ResourceKey="Brush-Success" />
	<StaticResource x:Key="Button.OutlineWarningBorderBrush" ResourceKey="Brush-Warning" />
	<StaticResource x:Key="Button.OutlineDangerBorderBrush" ResourceKey="Brush-Danger" />
	<!--  end Outline  -->
	<!--  Borderless -->
	<SolidColorBrush x:Key="Button.BorderlessBackground" Color="Transparent" />
	<SolidColorBrush x:Key="Button.BorderlessBorderBrush" Color="Transparent" />
	<!--  end Borderless  -->
	<StaticResource x:Key="Button.InputInnerForeground" ResourceKey="Brush-Text2" />
	<StaticResource x:Key="Button.InputInnerPointeroverForeground" ResourceKey="Brush-PrimaryPointerover" />
	<StaticResource x:Key="Button.InputInnerPressedForeground" ResourceKey="Brush-PrimaryActive" />
</ResourceDictionary>