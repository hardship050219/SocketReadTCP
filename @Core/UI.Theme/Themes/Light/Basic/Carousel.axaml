<ResourceDictionary xmlns="https://github.com/avaloniaui" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<SolidColorBrush x:Key="Carousel.ButtonForeground" Opacity="0.5" Color="{StaticResource Color-Black}" />
	<SolidColorBrush x:Key="Carousel.ButtonPointeroverForeground" Color="{StaticResource Color-Black}" />
	<SolidColorBrush x:Key="Carousel.IndicatorForeground" Opacity="0.5" Color="{StaticResource Color-Black}" />
	<SolidColorBrush x:Key="Carousel.IndicatorPointeroverForeground" Opacity="0.7" Color="{StaticResource Color-Black}" />
	<SolidColorBrush x:Key="Carousel.IndicatorPressedForeground" Color="{StaticResource Color-Black}" />
	<SolidColorBrush x:Key="Carousel.IndicatorSelectedForeground" Color="{StaticResource Color-Black}" />
</ResourceDictionary>